        $.extend(
        {
            redirectPost: function (location, args) {
                var form = $('<form>', { action: location, method: 'post' });
                $.each(args,
                     function (key, value) {
                         $(form).append(
                              $('<input>', { type: 'hidden', name: key, value: value })
