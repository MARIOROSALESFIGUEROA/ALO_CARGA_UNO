            redirectPostWinOpen: function (location, winName, args) {
                var form = $('<form>', { action: location, method: 'post', 'target': winName });
                $.each(args,
                     function (key, value) {
                         $(form).append(
                              $('<input>', { type: 'hidden', name: key, value: value })
